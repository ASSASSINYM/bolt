class Foo = read Bar {
  const f : int
}
read trait Bar {
  require const f : int
}
let x = new Foo(f:5) in 
  let y = 5 in 
  finish{
    (* can read aliases in different threads as neither are mutable *)
    async {
      begin
      x ;
      y
      end
    }
    async{
      begin
      x ;
      y
      end
    }
  } ;
  x.f
  end
end